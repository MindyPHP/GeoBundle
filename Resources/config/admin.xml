<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="mindy.bundle.geo.admin.city"
                 class="Mindy\Bundle\GeoBundle\Admin\CityAdmin">
            <tag name="admin.admin" slug="geo_city"/>
        </service>

        <service id="mindy.bundle.geo.admin.country"
                 class="Mindy\Bundle\GeoBundle\Admin\CountryAdmin">
            <tag name="admin.admin" slug="geo_country"/>
        </service>

        <service id="mindy.bundle.geo.admin.region"
                 class="Mindy\Bundle\GeoBundle\Admin\RegionAdmin">
            <tag name="admin.admin" slug="geo_region"/>
        </service>
    </services>
</container>